{
  "name": "superpeer-task-vue-storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-production": "npx http-server ./dist",
    "build": "vue-cli-service build",
    "build-with-dev": "vue-cli-service build --mode development",
    "build-with-ui": "vue-cli-service build && yarn build-storybook -o ./dist/ui",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "icon": "node ./tasks/svgv -s ./public/icons -t ./src/components/icons -j ./src/data/icons.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "sass-mq": "^5.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.22",
    "@storybook/vue": "^6.0.22",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "basename": "^0.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "svgo": "^1.3.2",
    "vue-template-compiler": "^2.6.11",
    "yargs": "^16.0.3"
